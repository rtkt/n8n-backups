    },
    {
      "parameters": {
        "jsCode": "let staticData = $getWorkflowStaticData('global').lastExecution;\nif ( ! staticData ) {\n  staticData = [];\n};\nfor (page of $input.all()) {\n  staticData.push(page.json.id);\n};\nif (staticData.length > 40) {\n  staticData = staticData.slice(staticData.length - 40 - 1);\n};\nreturn { json: {} };"
      },
